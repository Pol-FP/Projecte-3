;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.digicore-06.test. root.digicore-06.test. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; name server NS records
@	 IN	 NS         ns1.digicore-06.test.
ns1      IN      A          192.168.1.201
server   IN      A          192.168.1.201
dbserver IN      A          192.168.1.202
data     IN      CNAME      dbserver
